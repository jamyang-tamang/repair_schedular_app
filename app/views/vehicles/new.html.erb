<h2>Vehicles</h2>
<%= form_with(model: @vehicle, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :market_id %>
    <%= form.text_field :market_id %>
  </div>

  <div>
    <%= form.submit "Create Vehicle" , class: "btn btn-primary" %>
  </div>
<% end %>
